{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": { "files": ["/favicon.ico", "/*.html", "/*.css", "/*.js"] }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "resources": {
        "files": ["/assets/**", "/manifest.webmanifest", "/*.(png|jpg|jpeg|svg|webp)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["http://localhost:3000/**"],
      "cacheConfig": { "strategy": "freshness", "maxSize": 100, "maxAge": "1h", "timeout": "5s" }
    }
  ]
}